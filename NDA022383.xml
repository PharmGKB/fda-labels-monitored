      "clinical_studies_table": [
        "<table ID=\"t2\" width=\"100%\"> <caption>Table 2: LS Mean for trough FEV<sub>1</sub> at 12 weeks </caption> <col width=\"29.577%\" align=\"left\"/> <col width=\"35.212%\" align=\"left\"/> <col width=\"35.212%\" align=\"left\"/> <tbody> <tr> <td align=\"left\" valign=\"middle\" styleCode=\"Toprule Botrule\"> <content styleCode=\"bold\">Treatment</content> </td> <td align=\"center\" valign=\"middle\" styleCode=\"Toprule Botrule\"> <content styleCode=\"bold\">Trough FEV<sub>1</sub>at Week 12 (liters)</content> </td> <td align=\"center\" valign=\"middle\" styleCode=\"Toprule Botrule\"> <content styleCode=\"bold\">Treatment Difference</content>   <content styleCode=\"bold\">LS Mean (95% CI)</content> </td> </tr> <tr> <td colspan=\"3\" align=\"left\" valign=\"middle\" styleCode=\"Botrule\">Trial 4 (N=323) </td> </tr> <tr> <td align=\"left\" valign=\"middle\">Indacaterol 75 mcg </td> <td align=\"center\" valign=\"middle\">1.38 </td> <td align=\"center\" valign=\"middle\">0.12 (0.08, 0.15) </td> </tr> <tr> <td align=\"left\" valign=\"middle\" styleCode=\"Botrule\">Placebo </td> <td align=\"center\" valign=\"middle\" styleCode=\"Botrule\">1.26 </td> <td align=\"center\" valign=\"middle\" styleCode=\"Botrule\"/> </tr> <tr> <td colspan=\"3\" align=\"left\" valign=\"middle\" styleCode=\"Botrule\">Trial 5 (N=318) </td> </tr> <tr> <td align=\"left\" valign=\"middle\">Indacaterol 75 mcg </td> <td align=\"center\" valign=\"middle\">1.49 </td> <td align=\"center\" valign=\"middle\">0.14 (0.10, 0.18) </td> </tr> <tr> <td align=\"left\" valign=\"middle\" styleCode=\"Botrule\">Placebo </td> <td align=\"center\" valign=\"middle\" styleCode=\"Botrule\">1.35 </td> <td align=\"center\" valign=\"middle\" styleCode=\"Botrule\"/> </tr> </tbody> </table>"
