      "clinical_pharmacology_table": [
        "<table ID=\"t300\" width=\"100%\"> <caption>Outcome Classification (%) on CGI-Global Improvement Item for Completers in Study 1</caption> <col align=\"left\" width=\"32.140%\"/> <col align=\"left\" width=\"13.100%\"/> <col align=\"left\" width=\"16.660%\"/> <col align=\"left\" width=\"17.860%\"/> <col align=\"left\" width=\"20.240%\"/> <tbody> <tr> <td align=\"left\" styleCode=\"Toprule Botrule Lrule Rrule\" valign=\"bottom\"> <content styleCode=\"bold\">Outcome</content>   <content styleCode=\"bold\">Classification</content> </td> <td align=\"center\" styleCode=\"Toprule Botrule Rrule\" valign=\"bottom\"> <content styleCode=\"bold\">Placebo   </content> <content styleCode=\"bold\">(N=74)</content> </td> <td align=\"center\" styleCode=\"Toprule Botrule Rrule\" valign=\"bottom\"> <content styleCode=\"bold\">Paroxetine   </content> <content styleCode=\"bold\">20 mg   </content> <content styleCode=\"bold\">(N=75)</content> </td> <td align=\"center\" styleCode=\"Toprule Botrule Rrule\" valign=\"bottom\"> <content styleCode=\"bold\">Paroxetine   </content> <content styleCode=\"bold\">40 mg   </content> <content styleCode=\"bold\">(N=66)</content> </td> <td align=\"center\" styleCode=\"Toprule Botrule Rrule\" valign=\"bottom\"> <content styleCode=\"bold\">Paroxetine   </content> <content styleCode=\"bold\">60 mg   </content> <content styleCode=\"bold\">(N=66)</content> </td> </tr> <tr> <td align=\"left\" styleCode=\"Botrule Lrule Rrule\" valign=\"top\">Worse</td> <td align=\"center\" styleCode=\"Botrule Rrule\" valign=\"top\">14%</td> <td align=\"center\" styleCode=\"Botrule Rrule\" valign=\"top\">7%</td> <td align=\"center\" styleCode=\"Botrule Rrule\" valign=\"top\">7%</td> <td align=\"center\" styleCode=\"Botrule Rrule\" valign=\"top\">3%</td> </tr> <tr> <td align=\"left\" styleCode=\"Botrule Lrule Rrule\" valign=\"top\">No Change</td> <td align=\"center\" styleCode=\"Botrule Rrule\" valign=\"top\">44%</td> <td align=\"center\" styleCode=\"Botrule Rrule\" vali